{
  "name": "@seadub/danger-plugin-titanium-module",
  "description": "Eslint your code with Danger",
  "author": {
    "name": "Chris Williams",
    "email": "cwilliams@axway.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/sgtcoolguy/danger-plugin-titanium-module.git"
  },
  "bugs": {
    "url": "https://github.com/sgtcoolguy/danger-plugin-titanium-module/issues"
  },
  "homepage": "https://github.com/sgtcoolguy/danger-plugin-titanium-module#readme",
  "keywords": [
    "danger",
    "danger-plugin",
    "titanium"
  ],
  "version": "0.0.7",
  "main": "dist/index.js",
  "types": "types/index.d.ts",
  "scripts": {
    "commit": "git-cz",
    "build": "tsc",
    "test": "jest",
    "predocs": "rm -rf docs/",
    "docs": "esdoc -c .esdoc.json",
    "prepublish": "npm run build",
    "semantic-release": "semantic-release",
    "prettier": "prettier",
    "prettier-write": "npm run prettier -- --parser typescript --no-semi --trailing-comma es5 --write --print-width 120",
    "prettier-project": "npm run prettier-write -- 'src/**/*.{ts,tsx}'",
    "lint": "tslint \"src/**/*.ts\""
  },
  "license": "MIT",
  "engines": {
    "node": ">=10.0.0"
  },
  "devDependencies": {
    "@semantic-release/changelog": "^5.0.1",
    "@semantic-release/git": "^9.0.0",
    "@types/jest": "^24.0.9",
    "commitizen": "^4.2.1",
    "cz-conventional-changelog": "^3.3.0",
    "danger": "^9.2.10",
    "esdoc": "^1.1.0",
    "eslint": "^6.0.0",
    "husky": "^4.3.0",
    "jest": "^26.5.0",
    "lint-staged": "^10.4.0",
    "prettier": "^1.3.1",
    "semantic-release": "^17.1.2",
    "ts-jest": "^24.0.0",
    "tslint": "^6.1.3",
    "typescript": "^3.3.3333",
    "validate-commit-msg": "^2.12.1"
  },
  "optionalDependencies": {},
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "lint-staged": {
    "*.@(ts|tsx)": [
      "tslint --fix",
      "npm run prettier-write --"
    ]
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json"
    ],
    "transform": {
      ".(ts|tsx|js|json)": "ts-jest"
    },
    "testRegex": "(.test)\\.(ts|tsx)$",
    "testPathIgnorePatterns": [
      "\\.snap$",
      "<rootDir>/node_modules/"
    ]
  },
  "dependencies": {
    "fs-extra": "^8.1.0",
    "semver": "^7.1.1"
  },
  "husky": {
    "hooks": {
      "commit-msg": "validate-commit-msg",
      "pre-commit": "lint-staged"
    }
  },
  "release": {
    "branches": [
      "master"
    ],
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/changelog",
      "@semantic-release/npm",
      "@semantic-release/github",
      "@semantic-release/git"
    ],
    "preset": "angular"
  }
}
